<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee
         http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
         version="3.1">
  <filter>
    <filter-name>setCharacterEncodingFilter</filter-name>
    <filter-class>org.apache.catalina.filters.SetCharacterEncodingFilter</filter-class>
    <init-param>
      <param-name>encoding</param-name>
      <param-value>UTF-8</param-value>

    </init-param>
  </filter>
  <filter-mapping>
    <filter-name>setCharacterEncodingFilter</filter-name>
    <url-pattern>/*</url-pattern>
  </filter-mapping>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
    <servlet-name>modeServlet</servlet-name>
    <servlet-class>com.playdata.assignment.servlet.ModeServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>modeServlet</servlet-name>
    <url-pattern>/modeservlet</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>adminMain</servlet-name>
    <servlet-class>com.playdata.assignment.servlet.adminservlet.AdminMainServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>adminMain</servlet-name>
    <url-pattern>/adminmain</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>adminPreviewInsert</servlet-name>
    <servlet-class>com.playdata.assignment.servlet.adminservlet.PreviewInsertServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>adminPreviewInsert</servlet-name>
    <url-pattern>/adminpreviewinsert</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>adminCinemaInsert</servlet-name>
    <servlet-class>com.playdata.assignment.servlet.adminservlet.CinemaInsertServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>adminCinemaInsert</servlet-name>
    <url-pattern>/admincinemainsert</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>adminCombine1</servlet-name>
    <servlet-class>com.playdata.assignment.servlet.adminservlet.Combine1</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>adminCombine1</servlet-name>
    <url-pattern>/admincombine1</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>adminCombine2</servlet-name>
    <servlet-class>com.playdata.assignment.servlet.adminservlet.Combine2</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>adminCombine2</servlet-name>
    <url-pattern>/admincombine2</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>adminPrintPerson</servlet-name>
    <servlet-class>com.playdata.assignment.servlet.adminservlet.PersonPrintServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>adminPrintPerson</servlet-name>
    <url-pattern>/adminprintperson</url-pattern>
  </servlet-mapping>

</web-app>
